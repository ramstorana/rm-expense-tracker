{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "outDir": ".vercel/output/functions/api",
        "rootDir": ".",
        "resolveJsonModule": true,
        "declaration": false,
        "paths": {
            "../backend/*": [
                "./backend/*"
            ]
        }
    },
    "include": [
        "api/**/*",
        "backend/src/**/*"
    ],
    "exclude": [
        "node_modules",
        "frontend",
        "data"
    ]
}